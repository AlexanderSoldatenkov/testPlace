{
  // "parser": "@babel/eslint-parser",
  "extends": [
    // "eslint:recommended",
    "airbnb/base"
  ]
  ,
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  }
  ,
  // "parserOptions": {
  //   "ecmaVersion": 8,
  //   "sourceType": "module",
  //   "requireConfigFile": false
  // },
  "rules": {
    "semi": 2,
    "no-plusplus": "off",
    "eol-last": "off",
    "max-len": ["error", 150, {"ignoreUrls": true}],
    "no-tabs": "off",
    "arrow-body-style": "off",
    "no-alert": "off",
    "prefer-destructuring": "off",
    "indent": [2, "tab"],
    "no-param-reassign": "off"

  }
}
